{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": "off",
        "no-plusplus": "off"
    }
}
